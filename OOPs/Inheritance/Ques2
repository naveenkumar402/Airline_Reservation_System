class Vehicle{
	String color,fueltype;
	void engineStart() {
		System.out.println(color.toString());
		System.out.println(fueltype.toString());
		System.out.println("Engine Start\n");
	}
}
class Bike extends Vehicle{
	void engineStart(String color,String fueltype) {
		this.color=color;
		this.fueltype=fueltype;
		engineStart();
	}
}
class Car extends Vehicle{
	void engineStart(String color,String fueltype) {
		this.color=color;
		this.fueltype=fueltype;
		engineStart();
	}
}
public class Mock {
	public static void main(String []args) {
		Bike b=new Bike();
		Car c=new Car();
		b.engineStart("Black","Petrol");
		c.engineStart("Metallic blue","Diesel");
	}
}
